{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-unused-expressions": "off",
        "import/extensions": "off",
        "no-sequences": "off",
        "grouped-accessor-pairs": "off",
        "consistent-return": "off",
        "no-unused-vars": "off",
        "no-return-assign": "off",
        "no-shadow": "off",
        "max-len": "off",
        "no-restricted-syntax": "off"
    }
}
